//correnctness 100 : performance 62	
	function solution(A) {
		var aLen = A.length;
		var cnt = 0;
		for (var i = 0; i < aLen; i++) {
			var max1 = i + A[i];
			for (var j = i + 1; j < aLen; j++) {
				var min2 = j - A[j];

				if (min2 <= max1) {
					cnt++;
					if (cnt > 10,000,000) return -1;
				}
			}
		}
		return cnt;
	}
